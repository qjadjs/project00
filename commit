------------------개추------------------------
create table tbl_like(
LIKENO NUMBER NOT NULL PRIMARY KEY ,
BNO NUMBER,
user_ID VARCHAR2(50) NOT NULL,
LIKECHECK NUMBER DEFAULT 0 NOT NULL,
LIKEDATE DATE DEFAULT SYSDATE NOT NULL,
FOREIGN KEY (user_ID) REFERENCES tb_user(user_ID) ON DELETE CASCADE,
FOREIGN KEY (BNO) REFERENCES tbl_BOARD (BNO) ON DELETE CASCADE 
);


----------------------비추--------------------
create table tbl_dislike(
DISLIKENO NUMBER NOT NULL PRIMARY KEY ,
BNO NUMBER,
user_ID VARCHAR2(50) NOT NULL,
DISLIKECHECK NUMBER DEFAULT 0 NOT NULL,
DISLIKEDATE DATE DEFAULT SYSDATE NOT NULL,
FOREIGN KEY (user_ID) REFERENCES tb_user(user_ID) ON DELETE CASCADE,
FOREIGN KEY (BNO) REFERENCES tbl_BOARD (BNO) ON DELETE CASCADE 
);

tbl_board에 dislikecnt 추가(not null, 디폴트 0)